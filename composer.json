{
    "name": "vinsinity/laravelsteam",
    "description": "Laravel Steam",
    "keywords": ["steam", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Erhan Hangul",
            "email": "erhan@hangul.com"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "guzzlehttp/guzzle": "^6.3|^7.3",
        "illuminate/http": "^7.0|^8.0",
        "illuminate/cache": "^7.0|^8.0",
        "illuminate/support": "^7.0|^8.0",
        "ext-bcmath": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3.8"
    },
    "autoload": {
        "psr-4": {
            "Vinsinity\\LaravelSteam\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vinsinity\\LaravelSteam\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vinsinity\\LaravelSteam\\SteamProcessServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
